<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yedambnb.mapper.LodgingDetailMapper">
   <select id="selectLodging" parameterType="int" resultType="Lodging">
     SELECT *
     FROM tbl_lodging
     WHERE lodging_no = #{lodgingNo }
   </select>
</mapper>
